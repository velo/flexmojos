    <repositories>
        <repository>
            <id>rso</id>
            <url>http://repository.sonatype.org/content/groups/flexgroup/</url>
        </repository>
    </repositories>
    <pluginRepositories>
        <pluginRepository>
            <id>rso</id>
            <url>http://repository.sonatype.org/content/groups/flexgroup/</url>
